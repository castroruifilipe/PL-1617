Grammar

    0 $accept: Programa $end

    1 Programa: Funcoes MainFunc

    2 Funcoes: Funcoes Funcao
    3        | /* empty */

    4 Funcao: FUNCAO DESIGNACAO '(' ')' '{' Decls Instrucoes Return '}'

    5 Return: RETURN Expr ';'

    6 MainFunc: MAIN '{' Decls Instrucoes '}'

    7 Decls: Decls Decl
    8      | /* empty */

    9 Decl: VAR DeclsVar ';'

   10 DeclsVar: DeclVar
   11         | DeclVar ',' DeclsVar

   12 DeclVar: DESIGNACAO Init
   13        | DESIGNACAO '[' VALOR ']'
   14        | DESIGNACAO '[' VALOR ']' '[' VALOR ']'

   15 Init: '=' Expr
   16     | /* empty */

   17 Instrucoes: Instrucoes Instrucao
   18           | /* empty */

   19 Instrucao: Var '=' Expr ';'
   20          | IF '(' Cond ')' '{' Instrucoes '}'
   21          | IF '(' Cond ')' '{' Instrucoes '}' ELSE '{' Instrucoes '}'
   22          | WHILE '(' Cond ')' '{' Instrucoes '}'
   23          | PRINT Expr ';'
   24          | PRINT STRING ';'
   25          | READ Var ';'
   26          | DESIGNACAO '(' ')' ';'

   27 Cond: Expr '>' '=' Expr
   28     | Expr '<' '=' Expr
   29     | Expr '=' '=' Expr
   30     | Expr '!' '=' Expr
   31     | Expr '<' Expr
   32     | Expr '>' Expr
   33     | Expr

   34 Expr: Termo
   35     | Expr '+' Termo
   36     | Expr '-' Termo
   37     | Expr '|' Termo

   38 Termo: Fator
   39      | Termo '*' Fator
   40      | Termo '/' Fator
   41      | Termo '%' Fator
   42      | Termo '&' Fator

   43 Fator: VALOR
   44      | Var
   45      | '(' Cond ')'
   46      | DESIGNACAO '(' ')'

   47 Var: DESIGNACAO
   48    | DESIGNACAO '[' VALOR ']'
   49    | DESIGNACAO '[' VALOR ']' '[' VALOR ']'


