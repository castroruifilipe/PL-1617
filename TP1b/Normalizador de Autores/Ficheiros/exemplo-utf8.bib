@inproceedings{jspell2002,
 author= {Alberto M. Simáes and J.J. Almeida},
 title={ Jspell.pm -- um mádulo de análise morfolágica
para uso em processamento de linguagem natural},
 booktitle={Actas do XVII Encontro da Associaááo Portuguesa de Linguástica},
 address={Lisboa 2001},
 pages="485--495",
 abstract = {Neste documento á nosso propásito apresentar as
                caracterásticas presentes no analisador morfolágico
                jspell e quais as suas consequáncias ao nável de
                aplicaááes de processamento de linguagem natural. Como
                ferramenta que á frequentemente integrada em software
                mais especáfico, apresentamos um mádulo Perl
                desenvolvido com o objectivo de facilitar a interligaááo
                do analisador morfolágico com pequenas aplicaááes
                desenvolvidas em linguagens de scripting. Devido á
                constante necessidade de melhoramento de dicionários, e
                em particular dos analisadores morfolágicos, discutimos
                as propriedades que estes devem conter para facilitar o
                seu processamento e enriquecimento automático.},
 year= 2002,
}

